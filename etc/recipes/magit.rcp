(:name magit
       :description "It's Magit! An Emacs mode for Git."
       :website     "https://magit.vc"
       :depends     (compat cond-let llama seq transient with-editor)
       :type        github
       :pkgname     "magit/magit"
       :branch      "main"
       :info        "docs"
       :load-path   "lisp"
       :compile     "lisp"
       :build       `(("make" ,(concat "EMACS=" (shell-quote-argument el-get-emacs)) "info")))

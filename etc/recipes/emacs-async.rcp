(:name emacs-async
       :description "Simple library for asynchronous processing in Emacs"
       :website     "https://github.com/jwiegley/emacs-async"
       :type        github
       :pkgname     "jwiegley/emacs-async"
       :build       `(("make" ,(concat "EMACS_COMMAND=" (shell-quote-argument el-get-emacs))))
       :autoloads   nil
       :features    async-autoloads)

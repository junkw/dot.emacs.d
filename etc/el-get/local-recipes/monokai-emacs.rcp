(:name monokai-emacs
       :description "A fruity color theme for Emacs."
       :website     "https://github.com/oneKelvinSmith/monokai-emacs#readme"
       :type        github
       :pkgname     "oneKelvinSmith/monokai-emacs"
       :prepare     (progn
                      (add-to-list 'custom-theme-load-path default-directory)
                      (load-theme 'monokai t)))
